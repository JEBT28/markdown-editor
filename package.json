{
  "name": "markdown-editor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tailwindcss/jit": "^0.1.18",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "autoprefixer": "^10.2.5",
    "overwrite": "^1.0.3",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.1.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start && npm run dev ",
    "build": "npm run build:prod && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "TAILWIND_MODE=watch NODE_ENV=development postcss ./src/assets/tailwind.css -o ./src/assets/main.css -w",
    "build:prod": "TAILWIND_MODE=build NODE_ENV=production postcss ./src/assets/tailwind.css -o ./src/assets/main.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
